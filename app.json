{
  "expo": {
    "name": "Know Me Better",
    "slug": "know-me-better",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF5F7"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.knowmebetter.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./icon.png",
        "backgroundColor": "#FFF5F7"
      },
      "package": "com.knowmebetter.app",
      "versionCode": 1,
      "targetSdkVersion": 35,
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./icon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5a79e1f7-f9e6-45e4-a4df-355c32090829"
      }
    }
  }
}
